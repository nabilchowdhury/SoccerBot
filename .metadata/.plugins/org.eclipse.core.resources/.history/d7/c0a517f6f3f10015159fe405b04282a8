import lejos.hardware.sensor.EV3UltrasonicSensor;

public class TestUltrasonic {
	
	public static EV3UltrasonicSensor usSensor = new EV3UltrasonicSensor(SensorPort.S2);
	
	public static void main(String[] args){
		USPoller poller = new USPoller(usSensor);
		poller.start();
		
	}
}
